class Persegi(object):
	def __init__(self, p, l):
		self.panjang = p
		self.lebar = l
	def hitungLuas(self):
			return self.panjang * self.lebar
	def hitungKeliling(self):
			return self.panjang * 4
	def cetakData(self):
			print("panjang\t: ", self.panjang)
			print("lebar\t: ", self.lebar)
	def cetakLuas(self):
			print("Luas\t: ", self.hitungLuas())
	def cetakKeliling(self):
			print("Keliling: ", self.hitungKeliling())
			
def main():
	#membuat objek pertama
	kotak1 = Persegi(6, 6)
	
	#menggunakan objek pertama
	print("Objek persegi 1")
	kotak1.cetakData()
	kotak1.cetakLuas()
	kotak1.cetakKeliling()
	
	#membuat objek kedua
	kotak2 = Persegi(5, 5)
	
	#menggunakan objek kedua
	print("\nObjek persegi 2")
	kotak2.cetakData()
	kotak2.cetakLuas()
	kotak2.cetakKeliling()
if __name__ == "__main__":
	main()
	
	
class Lingkaran(object):
	def __init__(self, p, r):
		self.phi = p
		self.jarijari = r
	def hitungLuas(self):
			return self.phi * self.jarijari * self.jarijari
	def hitungKeliling(self):
			return 2 * self.phi * self.jarijari
	def cetakData(self):
			print("phi\t: ", self.phi)
			print("jari-jari: ", self.jarijari)
	def cetakLuas(self):
			print("Luas\t: ", self.hitungLuas())
	def cetakKeliling(self):
			print("Keliling :",self.hitungKeliling())
			
def main():
	#membuat objek pertama
	lingkaran1 = Lingkaran(22/7, 7)
	
	#menggunakan objek pertama
	print("\nObjek Lingkaran 1")
	lingkaran1.cetakData()
	lingkaran1.cetakLuas()
	lingkaran1.cetakKeliling()
	
	#membuat objek kedua
	lingkaran2 = Lingkaran(3.14, 5)
	
	#menggunakan objek kedua
	print("\nObjek Lingkaran 2")
	lingkaran2.cetakData()
	lingkaran2.cetakLuas()
	lingkaran2.cetakKeliling()
if __name__ == "__main__":
	main()

